{
	"schemaVersion": 1,
	"id": "vminus",
	"version": "${version}",
	"name": "VMinus",
	"description": "Check VMinus from Forge for this",
	"authors": [
		"Ether/Void"
	],
	"contact": {
		"homepage": "https://github.com/VoidAndBright",
		"sources": "https://github.com/VoidAndBright/VMinus-Fabric"
	},
	"license": "CC0-1.0",
	"icon": "assets/vminus/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.lixir.vminus.VMinus"
		],
		"client": [
			"net.lixir.vminus.VMinusClient"
		],
		"fabric-datagen": [
			"net.lixir.vminus.VMinusDataGenerator"
		],
		"modmenu": [
			"net.lixir.vminus.VMinusModMenu"
		]
	},
	"mixins": [
		"vminus.mixins.json"
	],
	"accessWidener" : "vminus.accesswidener",
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	},
	"custom": {
		"modmenu": {
			"badges": ["library"],
			"update_checker": true
		}
	}
}